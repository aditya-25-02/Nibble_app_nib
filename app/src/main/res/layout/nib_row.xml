<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nib_one_row"
    android:layout_width="match_parent"
    android:layout_height="272dp"
    android:layout_marginTop="30dp"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginBottom="10dp"
    app:cardCornerRadius="20dp"
    android:padding="5dp"
    android:background="#FFFFFF"
    >
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/nib_image"
            android:layout_width="83dp"
            android:layout_height="69dp"
            app:layout_constraintBottom_toBottomOf="@+id/nib_club"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.116"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/nib_name"
            app:srcCompat="@drawable/ic_launcher_background" />

    <TextView
        android:id="@+id/nib_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginLeft="72dp"
        android:text="Name"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.537"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.067" />

        <TextView
            android:id="@+id/nib_club"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Club"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/nib_name"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.297" />

        <TextView
            android:id="@+id/nib_work"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Programmer"
            android:textSize="15sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/nib_club"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.198" />

        <TextView
            android:id="@+id/nib_year"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="I - YEAR"
            android:textSize="25dp"
            app:layout_constraintBottom_toBottomOf="@+id/nib_mail"
            app:layout_constraintEnd_toEndOf="@+id/nib_image"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/nib_image"
            app:layout_constraintTop_toTopOf="@+id/nib_call"
            app:layout_constraintVertical_bias="0.314" />

        <ImageView
        android:id="@+id/nib_call"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nib_work"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.599"
        app:srcCompat="@drawable/ic_logo_call" />

    <ImageView
        android:id="@+id/nib_linkedIn"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/nib_call"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.264"
        app:layout_constraintStart_toEndOf="@+id/nib_call"
        app:layout_constraintTop_toTopOf="@+id/nib_call"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_logo_linkedin" />

    <ImageView
        android:id="@+id/nib_git"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/nib_linkedIn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/nib_linkedIn"
        app:layout_constraintTop_toTopOf="@+id/nib_linkedIn"
        app:srcCompat="@drawable/ic_logo_github" />

    <ImageView
        android:id="@+id/nib_mail"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/nib_call"
        app:layout_constraintTop_toBottomOf="@+id/nib_call"
        app:srcCompat="@drawable/ic_logo_mail" />

    <ImageView
        android:id="@+id/nib_cc"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/nib_mail"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toEndOf="@+id/nib_mail"
        app:layout_constraintTop_toBottomOf="@+id/nib_linkedIn"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/ic_logo_cc" />

    <ImageView
        android:id="@+id/nib_cf"
        android:layout_width="30dp"
        android:layout_height="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/nib_cc"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/nib_git"
        app:layout_constraintTop_toTopOf="@+id/nib_cc"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_logo_cf" />

    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>
